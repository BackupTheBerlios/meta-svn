|
	default=
		|
			defaultArgument=arg.argument
			func=arg.function
			|func merge
				defaultArgument
				arg
	todoness=default
		function=
			|if
				condition=equal
					length arg.dependencies
					0
				then=
					|divide
						arg.arg.[3]
						arg.arg.[2]
				else=
					|subtract
						todoness features.[arg.arg.dependencies.[1]]
						1/100000
		argument=
			dependencies=*
			[1]=*
			[2]=1
			[3]=0
	apply
		array=sort
			array=foreach
				map=features
				function=
					|merge
						arg.value
						=
							todo=todoness arg.value
							id=arg.key
			compare=
				|greater
					arg.a.todo
					arg.b.todo
		function=default
			function=
				|console.writeLine join
					integerToString arg.id
					" "
					integerToString arg.[2]
					" "
					integerToString todoness arg
					" "
					arg.[1]
			argument=
				*
				1
				0
	console.readLine *