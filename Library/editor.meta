window=Window
	title="Meta Editor"
	menu=
		File=
			["Open..."]=!OpenFile""
			["Save"]=Save
			["Save as..."]=SaveAs
			["Close"]=!window.Close""
		Program=
			Start=RunProgram
			Continue=ContinueProgram
			Reverse=ReverseProgram
	font=Font
		"Courier New"
		"10"
	opacity=
		numerator="10"
		denominator="10"
	WindowState=FormWindowState.Maximized
	controls=
		editor
window.Show""