|
	result 0
	. apply
		. string.split
			text ["INPUT.txt"]
			delimiters
				. characters.linefeed
		line|
			parts string.split
				text line
				delimiters
					. characters.tab
			value parts.[1]
			key parts.[2]
			$result.[key] if
				. contains
					map result
					key key
				then :append
					. result.[key]
					.
						. value
				else :
					. value
	. foreach
		. result
		entry|
			$result.[entry.key] sort
				. entry.value
				arg|string.compare
					. arg.[1]
					. arg.[2]
	sortedKeys sort
		. keys result
		arg|string.compare
			. arg.[1]
			. arg.[2]
	sortedOutput 0
	. apply
		. sortedKeys
		key|
			$sortedOutput.[key] result.[key]
	current sortedOutput