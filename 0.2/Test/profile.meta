|
	result foreach
		. invert apply
			. string.split
				text ["INPUT.txt"]
				delimiters
					. characters.linefeed
			line|
				parts string.split
					text line
					delimiters
						. characters.tab
				current
					[parts.[2]] parts.[1]
		entry|sort
			array entry.value
			function
				arg|string.compare
					. arg.[1]
					. arg.[2]
	current apply
		. sort
			array keys result
			function
				arg|string.compare
					. arg.[1]
					. arg.[2]
		key|append
			. 
				. key
			. result.[key]