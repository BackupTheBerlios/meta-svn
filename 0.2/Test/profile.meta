|
	lines string.split
		. ["INPUT.txt"]
		delimiters
			. characters.linefeed
	result 0
	. apply
		. lines
		line|
			parts string.split
				. line
				delimiters
					. characters.tab
			value parts.[1]
			key parts.[2]
			. if
				. contains
					map result
					key key
				then :
					$result.[key] append
						. result.[key]
						.
							. value
				else :
					$result.[key]
						. value
	. foreach
		. result
		entry|
			$result.[entry.key] sort
				. entry.value
				arg|string.compare
					. arg.[1]
					. arg.[2]
	sortedKeys sort
		. keys result
		arg|string.compare
			. arg.[1]
			. arg.[2]
	sortedOutput 0
	. apply
		. sortedKeys
		key|
			$sortedOutput.[key] result.[key]
	current sortedOutput