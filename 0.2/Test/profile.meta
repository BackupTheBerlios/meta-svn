|
	result foreach
		. invert apply
			array string.split
				text ["INPUT.txt"]
				delimiters
					. characters.linefeed
			function
				line|
					parts string.split
						text line
						delimiters
							. characters.tab
					current
						[parts.[2]] parts.[1]
		entry|sort
			array entry.value
			function
				arg|string.compare
					. arg.[1]
					. arg.[2]
	current apply
		array sort
			array keys result
			function
				arg|string.compare
					. arg.[1]
					. arg.[2]
		function
			key|append
				. 
					. key
				. result.[key]