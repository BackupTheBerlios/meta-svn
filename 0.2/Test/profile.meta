|
	lines string.split
		. ["INPUT.txt"]
		delimiters
			. characters.linefeed
	result 0
	. apply
		. lines
		line|
			parts string.split
				. line
				delimiters
					. characters.tab
			key parts.[1]
			value parts.[2]
			if
				. contains
					map result
					key key
				then :
					$result.[key] append
						. result.[key]
						.
							. value
				else :
					$result.[key]
						. value
	current result