		Map (range 10) ?bubble (random 2)
	-
		add_Tick_TickEventHandler
		??If (not paused) ?,
			(contactGroup.Empty
			(Video.get_Screen).Fill_Color (Color.get_White
			(space.CheckCollisions
			(world.Step
			Map bubbles b?,
				(b.move
				(b.draw
			Map players player?(player.draw
			Map players player?If (not (equal 0 player.currentJoint)) ?,
				a=(player.currentJoint.get_Anchor
				b=(player.body.get_Position
				DrawLine_Line_Color (Line_Int16_Int16_Int16_Int16 (a.get_X) (a.get_Y) (b.get_X) (b.get_Y)) (Color.get_Black
			(Flip
	(Run