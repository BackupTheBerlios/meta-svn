		SdlDotNet.Key
		OdeDotNet
	world=(World
	space=(Space
	createBox=space.CreateBox_Single_Single_Single
	vector=Vector3_Single_Single_Single
	(createBox 800 20 20).set_Position_Vector3 (vector 400 0 0)
	(createBox 800 20 20).set_Position_Vector3 (vector 400 600 0)
	(createBox 20 600 20).set_Position_Vector3 (vector 0 300 0)
	(createBox 20 600 20).set_Position_Vector3 (vector 800 300 0)
	bubble=size?,
